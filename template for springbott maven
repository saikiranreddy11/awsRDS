- name: Export Environment Variables
  run: |
    echo "TF_VAR_deployDomainMqu=${DEPLOYDOMAINMQU}" >> $GITHUB_ENV
    echo "TF_VAR_deployDomainT7sgsc=${DEPLOYDOMAINT7SGSC}" >> $GITHUB_ENV
    echo "TF_VAR_deployDomainStarlims=${DEPLOYDOMAINSTARLIMS}" >> $GITHUB_ENV
    echo "TF_VAR_deployDomainRootIdService=${DEPLOYDOMAINROOTIDSERVICE}" >> $GITHUB_ENV
    echo "TF_VAR_deployDomainReportingService=${DEPLOYDOMAINREPORTINGSERVICE}" >> $GITHUB_ENV
    echo "TF_VAR_deployDomainQdrs=${DEPLOYDOMAINQDRS}" >> $GITHUB_ENV
    echo "TF_VAR_deployDomainQuestPingFederate=${DEPLOYDOMAINQUESTPINGFEDERATE}" >> $GITHUB_ENV
    echo "TF_VAR_backendSecretKey=${BACKENDSECRETKEY}" >> $GITHUB_ENV
    echo "TF_VAR_frontendSecretKey=${FRONTENDSECRETKEY}" >> $GITHUB_ENV
    echo "TF_VAR_reportingServiceKey=${REPORTINGSERVICEKEY}" >> $GITHUB_ENV
    echo "TF_VAR_APP_URL=${APP_URL}" >> $GITHUB_ENV
    echo "TF_VAR_XDEBUG_MODE=${XDEBUG_MODE}" >> $GITHUB_ENV
    echo "TF_VAR_APP_KEY=${APP_KEY}" >> $GITHUB_ENV
    echo "TF_VAR_DB_DATABASE=${DB_DATABASE}" >> $GITHUB_ENV
    echo "TF_VAR_DB_DATABASE_XREF_UDF=${DB_DATABASE_XREF_UDF}" >> $GITHUB_ENV
    echo "TF_VAR_DB_CONNECTION=${DB_CONNECTION}" >> $GITHUB_ENV
    echo "TF_VAR_SESSION_DOMAIN=${SESSION_DOMAIN}" >> $GITHUB_ENV
    echo "TF_VAR_SANCTUM_STATEFUL_DOMAINS=${SANCTUM_STATEFUL_DOMAINS}" >> $GITHUB_ENV
    echo "TF_VAR_MEMCACHED_HOST=${MEMCACHED_HOST}" >> $GITHUB_ENV
    echo "TF_VAR_REDIS_HOST=${REDIS_HOST}" >> $GITHUB_ENV
    echo "TF_VAR_REDIS_PASSWORD=${REDIS_PASSWORD}" >> $GITHUB_ENV
    echo "TF_VAR_REDIS_PORT=${REDIS_PORT}" >> $GITHUB_ENV
    echo "TF_VAR_MAIL_HOST=${MAIL_HOST}" >> $GITHUB_ENV
    echo "TF_VAR_MAIL_PORT=${MAIL_PORT}" >> $GITHUB_ENV
    echo "TF_VAR_MAIL_USERNAME=${MAIL_USERNAME}" >> $GITHUB_ENV
    echo "TF_VAR_MAIL_PASSWORD=${MAIL_PASSWORD}" >> $GITHUB_ENV
    echo "TF_VAR_LDAP_HOST=${LDAP_HOST}" >> $GITHUB_ENV
    echo "TF_VAR_LDAP_USERNAME=${LDAP_USERNAME}" >> $GITHUB_ENV
    echo "TF_VAR_LDAP_PASSWORD=${LDAP_PASSWORD}" >> $GITHUB_ENV
    echo "TF_VAR_LDAP_PORT=${LDAP_PORT}" >> $GITHUB_ENV
    echo "TF_VAR_FRONTEND_URL=${FRONTEND_URL}" >> $GITHUB_ENV
    echo "TF_VAR_BACKEND_CLIENT_URL=${BACKEND_CLIENT_URL}" >> $GITHUB_ENV
    echo "TF_VAR_BACKEND_SERVER_URL=${BACKEND_SERVER_URL}" >> $GITHUB_ENV
    echo "TF_VAR_NEXAUTH_URL=${NEXAUTH_URL}" >> $GITHUB_ENV
    echo "TF_VAR_NEXAUTH_URL_INTERNAL=${NEXAUTH_URL_INTERNAL}" >> $GITHUB_ENV
    echo "TF_VAR_NEXAUTH_SECRET=${NEXAUTH_SECRET}" >> $GITHUB_ENV
    echo "TF_VAR_SERVER_SESSION_COOKIE_NAME=${SERVER_SESSION_COOKIE_NAME}" >> $GITHUB_ENV
    echo "TF_VAR_HOSTNAME=${HOSTNAME}" >> $GITHUB_ENV
